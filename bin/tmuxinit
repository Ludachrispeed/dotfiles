#!/usr/bin/env bash

# Sesison Home
# ------------

tmux new-session -d -s Home
tmux new-window -d -n clojure "ls -alh"

# Session Work
# ------------

tmux new-session -d -s Work
tmux new-window -d -c ~/IronNet/irondefense-orchestration/ikcm -n ikcm "ls -alh"

# https://github.com/tmux-plugins/tmux-copycat/issues/116
tmux unbind -n n \; unbind -n N
